<?xml version="1.0" encoding="UTF-8"?>
 
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <!--
        See http://www.liquibase.org/manual/home#available_database_refactorings
        for a list of supported elements and attributes
    -->
    

    <changeSet author="israelRukshin2" id="20131010-8">
        <comment>Create the department table</comment>
        <createTable tableName="personalization_personalization2">
             <column autoIncrement="true" name="personalization_id" type="int">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="breakfast" type="varchar(255)" />
            <column name="lunch" type="varchar(255)" />
            <column name="evening" type="varchar(255)" />
            <column name="breakfastAlarm" type="varchar(255)" />
            <column name="lunchAlarm" type="varchar(255)" />
            <column name="eveningAlarm" type="varchar(255)" />
            <column name="day" type="varchar(255)" />
            <column name="day1" type="varchar(255)" />
            <column name="day2" type="varchar(255)" />
            <column name="time" type="varchar(255)" />
            <column name="time2" type="varchar(255)" />
            <column name="uuid" type="char(38)" />
        </createTable>
    </changeSet>
    
</databaseChangeLog>